<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd" >
<beans>

	<alias name="gus" alias="gus2"/>
	
	<bean id="gus1" 
		class="org.springframework.jdbc.datasource.SingleConnectionDataSource"
		destroy-method="close">
		
		<property name="driverClassName" value="oracle.jdbc.driver.OracleDriver" />
		<property name="url" value="jdbc:oracle:thin:@ocs3:1534:pat" />
		<property name="username" value="gus1" />
		<property name="password" value="genedb1" />
	</bean>

	<bean id="gus2" 
		class="org.springframework.jdbc.datasource.SingleConnectionDataSource"
		destroy-method="close">
		
		<property name="driverClassName" value="oracle.jdbc.driver.OracleDriver" />
		<property name="url" value="jdbc:oracle:thin:@ocs3:1534:pat" />
		<property name="username" value="gus2" />
		<property name="password" value="genedb2" />
	</bean>
	
</beans>
